<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title type="text" xml:lang="en">i.webthings.hub directory feed</title>
  <link type="application/atom+xml" href="https://iwebthings.joejenett.com/directory/feed.atom" rel="self"/>
  <link type="text/html" href="https://iwebthings.joejenett.com/" rel="alternate"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://iwebthings.joejenett.com/i_webthings_directory_feed</id>
  <author>
    <name>joe jenett</name>
  </author>
  <rights>2010-2023 Â© joe jenett. Some rights reserved.</rights>

  {% for post in site.posts %}
  {% if post in site.categories[category_name] == "i-webthings-directory" limit:1 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="https://iwebthings.joejenett.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>https://iwebthings.joejenett.com{{ post.url }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  <% endif %}
  {% endfor %}
</feed>

